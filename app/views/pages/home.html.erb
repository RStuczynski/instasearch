<div class="jumbotron center">
   <h1><i class="fa fa-instagram fa-1x"></i>Stew's Clues</h1>  
   <p>Instagram Curiosity Satisfied</p>
   <br>
   <form class="col-lg-12">
      <div class="input-group input-group-lg col-sm-offset-4 col-sm-4">
         <input id="search" type="text" name="q" class="center-block form-control input-lg"
         title="Search Photos" placeholder="Search Photos">
         <span class="input-group-btn"><button class="btn btn-lg
         btn-primary" type="submit">GO!</button></span>
      </div>
   </form>
   <br>
</div>
<div class="container">
   
   
    <% @search.each do |instagram| %>
       <div class="row">
       <%= image_tag instagram.images.low_resolution.url %>
       </div>
    <% end %>
    
</div>